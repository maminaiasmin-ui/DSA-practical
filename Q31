#include <stdio.h>

int main() {
    int n, k;

    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter position (n-th element to return): ");
    scanf("%d", &k);

    if (k <= 0 || k > n) {
        printf("Invalid position!\n");
    } else {
        printf("The %d-th element is: %d\n", k, arr[k-1]);
    }

    return 0;
}
